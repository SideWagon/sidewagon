{
  "name": "sidewagon",
  "version": "1.0.0",
  "description": "A site for SideWagon LLC",
  "main": "index.js",
  "scripts": {
    "build-css": "stylus source/stylesheets/index.styl -o static/css",
    "watch-css": "stylus source/stylesheets/index.styl -o static/css -w",
    "clean": "rm -rf static/css && mkdir -p static/css",
    "build": "npm run clean && npm run build-css",
    "watch": "npm run clean && npm run watch-css & nodemon server -e js,jade",
    "start": "node server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SideWagon/sidewagon.git"
  },
  "keywords": [
    "sidewagon"
  ],
  "author": "James Fan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SideWagon/sidewagon/issues"
  },
  "homepage": "https://github.com/SideWagon/sidewagon#readme",
  "dependencies": {
    "express": "^4.16.2",
    "jade": "^1.11.0",
    "nodemon": "^1.9.2",
    "stylus": "^0.54.5"
  }
}
